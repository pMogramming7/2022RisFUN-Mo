equ <- function(x1,x2){
gr <-1.1
num_gen <- 30
generation <- 1:num_gen
N1 <- rep (0, num_gen)
N2 <- rep (0, num_gen)
a12 <- 0.6
a21 <- 1.2
K1 <- 100
K2 <- 120
N1[1] <- x1
N2[1] <- x2
for (i in 2:num_gen) {
N1[i] = N1[i-1] + (3.2* N1[i-1] * ((K1-N1[i-1]-(a12*N2[i-1]))/K1))
N2[i] = N2[i-1] + (gr* N2[i-1] * ((K2-N2[i-1]-(a21*N1[i-1]))/K2))
generation[1]=1
print (N1[i])
if (N1[1]>0){
plot(N1~generation,typ="b",ylim=c(0,min(c(K1+10,K2+10))),ylab="N")
}  else {
plot(N1~generation,typ="n",ylim=c(0,min(c(K1+10,K2+10))),ylab="N")
}
print(N2[i])
if (N2[1]>0){
lines(N2~generation,typ="b",col=3,ylim=c(0,min(c(K1+10,K2+10))),ylab="N")} else {
plot(N2~generation,typ="n",ylim=c(0,min(c(K1+10,K2+10))),ylab="N")
}
}
}
saveGIF({
equ(6,6)
}, interval = 0.1, movie.name = "competing_model.gif")
saveGIF({
equ(6,6)
}, interval = 0.1, movie.name = "competing_model.gif")
