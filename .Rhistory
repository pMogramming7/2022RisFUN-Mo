grow.r.g <- function (growth.rate, number.generation) {
num_gen<-number.generation
generation<-1:num_gen
N <- rep (0,num_gen)
N[1] <- 1
for (i in 2:num_gen) {
N[i]=growth.rate*N[i-1]
}
plot(N~generation,type='b', main=paste("Rate =", growth.rate, ", ", number.generation, "generations"))
}
